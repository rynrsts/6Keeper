<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="?android:attr/actionBarSize"
    tools:context=".SecurityQuestionFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scrollbarThumbVertical="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="30dp"
            android:paddingBottom="30dp">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/gSecurityQuestionVertical09"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".09" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/gSecurityQuestionVertical91"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.91" />

            <TextView
                android:id="@+id/tvSecurityQuestionNote"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/security_question_mes"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvSecurityQuestionFirstName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:text="@string/many_first_name"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/tvSecurityQuestionNote" />

            <EditText
                android:id="@+id/etSecurityQuestionFirstName"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/layout_edit_text"
                android:inputType="textPersonName"
                android:maxLength="30"
                android:paddingStart="48dp"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/tvSecurityQuestionFirstName"
                tools:ignore="Autofill, LabelFor, RtlSymmetry" />

            <ImageView
                android:id="@+id/ivSecurityQuestionFirstName"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:padding="12dp"
                android:src="@drawable/ic_person_gray"
                app:layout_constraintBottom_toBottomOf="@+id/etSecurityQuestionFirstName"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toTopOf="@+id/etSecurityQuestionFirstName"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tvSecurityQuestionLastName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:text="@string/many_last_name"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/etSecurityQuestionFirstName" />

            <EditText
                android:id="@+id/etSecurityQuestionLastName"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/layout_edit_text"
                android:inputType="textPersonName"
                android:maxLength="30"
                android:paddingStart="48dp"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/tvSecurityQuestionLastName"
                tools:ignore="Autofill, LabelFor, RtlSymmetry" />

            <ImageView
                android:id="@+id/ivSecurityQuestionLastName"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:padding="12dp"
                android:src="@drawable/ic_person_gray"
                app:layout_constraintBottom_toBottomOf="@+id/etSecurityQuestionLastName"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toTopOf="@+id/etSecurityQuestionLastName"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tvSecurityQuestionBirthDate"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:text="@string/many_birth_date"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/etSecurityQuestionLastName" />

            <EditText
                android:id="@+id/etSecurityQuestionBirthDate"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/layout_edit_text"
                android:hint="@string/many_birth_date_format"
                android:inputType="text"
                android:maxLength="10"
                android:paddingStart="48dp"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/tvSecurityQuestionBirthDate"
                tools:ignore="Autofill, LabelFor, RtlSymmetry, TextFields" />

            <ImageView
                android:id="@+id/ivSecurityQuestionBirthDate"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:padding="12dp"
                android:src="@drawable/ic_date_range_gray"
                app:layout_constraintBottom_toBottomOf="@+id/etSecurityQuestionBirthDate"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toTopOf="@+id/etSecurityQuestionBirthDate"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tvSecurityQuestionEmail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:text="@string/many_email"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/etSecurityQuestionBirthDate" />

            <EditText
                android:id="@+id/etSecurityQuestionEmail"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/layout_edit_text"
                android:hint="@string/many_email_hint"
                android:inputType="textEmailAddress"
                android:maxLength="60"
                android:paddingStart="48dp"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/tvSecurityQuestionEmail"
                tools:ignore="Autofill, LabelFor, RtlSymmetry" />

            <ImageView
                android:id="@+id/ivSecurityQuestionEmail"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:padding="12dp"
                android:src="@drawable/ic_email_gray"
                app:layout_constraintBottom_toBottomOf="@+id/etSecurityQuestionEmail"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toTopOf="@+id/etSecurityQuestionEmail"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tvSecurityQuestionMobileNumber"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:text="@string/many_mobile_number"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/etSecurityQuestionEmail" />

            <EditText
                android:id="@+id/etSecurityQuestionMobileNumber"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/layout_edit_text"
                android:hint="@string/many_mobile_number_hint"
                android:inputType="number"
                android:maxLength="10"
                android:paddingStart="84dp"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/tvSecurityQuestionMobileNumber"
                tools:ignore="Autofill, LabelFor, RtlSymmetry, TextFields" />

            <ImageView
                android:id="@+id/ivSecurityQuestionMobileNumber"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:padding="12dp"
                android:src="@drawable/ic_phone_gray"
                app:layout_constraintBottom_toBottomOf="@+id/etSecurityQuestionMobileNumber"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toTopOf="@+id/etSecurityQuestionMobileNumber"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/tvSecurityQuestionMobileNumberPrefix"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:gravity="center"
                android:text="@string/many_mobile_number_prefix"
                app:layout_constraintBottom_toBottomOf="@+id/etSecurityQuestionMobileNumber"
                app:layout_constraintStart_toEndOf="@+id/ivSecurityQuestionMobileNumber"
                app:layout_constraintTop_toTopOf="@+id/etSecurityQuestionMobileNumber" />

            <View
                android:id="@+id/vCreateNewAccP4Division1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="27dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintTop_toBottomOf="@+id/etSecurityQuestionMobileNumber" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/acbSecurityQuestionConfirm"
                android:layout_width="0dp"
                android:layout_height="48dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/layout_button_green_quadrilateral"
                android:stateListAnimator="@null"
                android:text="@string/security_question_confirm"
                android:textAllCaps="false"
                android:textColor="@color/white"
                app:layout_constraintEnd_toStartOf="@+id/gSecurityQuestionVertical91"
                app:layout_constraintStart_toStartOf="@+id/gSecurityQuestionVertical09"
                app:layout_constraintTop_toBottomOf="@+id/vCreateNewAccP4Division1" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>